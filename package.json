{
  "name": "jc-forecast",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://jchritton.github.io/jc-forecast",
  "repository": {
    "type": "git",
    "url": "https://github.com/jchritton/jc-forecast"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "start": "npm-run-all -p watch:sass start-react",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --fix src",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "sass": "node-sass-chokidar --output-style expanded --source-map-embed true src/sass/style.scss -o src/css",
    "watch:sass": "node-sass-chokidar src/sass -o src/css --watch --recursive"
  },
  "dependencies": {
    "gh-pages": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.22.2",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-animated-weather": "^1.0.3",
    "react-dom": "^16.5.2",
    "react-places-autocomplete": "^7.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-transition-group": "^2.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "faker": "^4.1.0",
    "node-sass-chokidar": "1.3.4",
    "npm-run-all": "^4.1.3",
    "prettier-eslint": "^8.8.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
